{
  "name": "api-end-to-end-test",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:dev": "tsc --project tsconfig.json --watch",
    "postbuild": "cp package.json dist/package.json && cp yarn.lock dist/yarn.lock && cd ./dist && yarn install --production"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.799.0",
    "@aws-sdk/client-lambda": "3.799.0",
    "@aws-sdk/lib-dynamodb": "3.799.0",
    "axios": "^1.9.0",
    "jose": "^6.0.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/node": "^22.15.3",
    "tsc-alias": "^1.8.15",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.1"
}
