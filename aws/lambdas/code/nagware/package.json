    "notifications-node-client": "5.2.3",
    "pg": "8.10.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.128",
    "@types/node": "^20.9.4",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.2"
  }
}
